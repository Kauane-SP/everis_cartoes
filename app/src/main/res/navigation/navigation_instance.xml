<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_instance"
    app:startDestination="@id/login">
    <fragment
        android:id="@+id/login"
        android:name="com.example.cartoeseveris.fragments.Login"
        android:label="Login" >
        <action
            android:id="@+id/action_login_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_login_to_home2"
            app:destination="@id/home2" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.cartoeseveris.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/home2"
        android:name="com.example.cartoeseveris.fragments.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>